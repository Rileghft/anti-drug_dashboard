		<div class="row">
			<div class="col-md-12">
				<div class="maintitle">
					<h3 class="section-title">資料視覺化設定</h3>
				</div>
				<div class="data">
					<h4 >各年齡層毒品資料</h4>
					<table>
						<tbody class="first">
							<tr>
							<td>描述</td>
							<td><%= data.description %></td>
							</tr>
							<tr>
							<td>資料集提供機關名稱</td>
							<td><%= data.provider %></td>
							</tr>
							<tr>
							<td>資料類型</td>
							<td><%= data.dataType %></td>
							</tr>
							<tr>
							<td>轉換後資料類型</td>
							<td><%= data.convertDataType %></td>
							</tr>
							<tr>
							<td>資料更新頻率</td>
							<td><%= data.updateFreq %></td>
							</tr>
							<tr>
							<td>資料開始收集時間</td>
							<td><%= data.dateUnit %> <%= data.dateFrom %></td>
							</tr>
							<tr>
							<td>資料來源</td>
							<td><%= data.source %></td>
							</tr>
						</tbody>
					</table>
				</div>
				<div class="data">
					<h4 >欄位說明</h4>
					<table>
						<tbody class="second">
							<tr>
								<th>欄位</th>
								<th>中文名稱</th>
								<th>資料類型</th>
								<th>單位</th>
							</tr>
                            <% data.column.forEach(function(element) { %>
							<tr>
							<td><%= element.name %></td>
							<td><%= element.description %></td>
							<td><%= element.type %></td>
							<td><%= element.unit %></td>
							</tr>
							<%});%>

						</tbody>
					</table>
				</div>

				<form id="visualSettinge" action="/api/dataVisual" method="post">
					<h4>視覺化選項</h4>
					<fieldset>
						<legend>圖表類型</legend>
						<label for="bar">長條圖</label>
						<input type="radio" name="type" id="bar" value="bar">
						<label for="line">折線圖</label>
						<input type="radio" name="type" id="line" value="line">
						<label for="scatter">XY散布圖</label>
						<input type="radio" name="type" id="scatter" value="scatter">
					</fieldset>
					<fieldset>
						<legend>X軸資料欄位</legend>
						<label for="columnX1">資料欄位1</label>
						<input type="radio" name="x" id="columnX1" value="資料欄位1">
						<label for="columnX2">資料欄位2</label>
						<input type="radio" name="x" id="columnX2" value="資料欄位2">
						<label for="columnX3">資料欄位3</label>
						<input type="radio" name="x" id="columnX3" value="資料欄位3">
						<label for="columnX4">資料欄位4</label>
						<input type="radio" name="x" id="columnX4" value="資料欄位4">
					</fieldset>
					<fieldset>
						<legend>Y軸資料欄位</legend>
						<label for="columnY1">資料欄位1</label>
						<input type="radio" name="y" id="columnY1" value="資料欄位1">
						<label for="columnY2">資料欄位2</label>
						<input type="radio" name="y" id="columnY2" value="資料欄位2">
						<label for="columnY3">資料欄位3</label>
						<input type="radio" name="y" id="columnY3" value="資料欄位3">
						<label for="columnY4">資料欄位4</label>
						<input type="radio" name="y" id="columnY4" value="資料欄位4">
					</fieldset>
					<input class="submit" type="submit" name="submit" value="送出" />
					<input class="submit" type="submit" name="revised" value="修改" />
				</form>
